using System.Diagnostics.CodeAnalysis;
using System.Net;
using System.Xml.Schema;

namespace _5._Add_and_Subtract
{
    internal class Program
    {
        static void Main(string[] args)
        {
           int num1 = int.Parse(Console.ReadLine());
           int num2 = int.Parse(Console.ReadLine());
           int num3 = int.Parse(Console.ReadLine());
           int sum = SumOfFirstTwoNumbers(num1, num2);
           int result = OutputResultSubtractsTheThirdOfTheFirstTwo(sum, num3);
           Console.WriteLine(result);
        }
        static int OutputResultSubtractsTheThirdOfTheFirstTwo(int sum, int num3)
        {
            return sum - num3;
        }
        static int SumOfFirstTwoNumbers(int num1, int num2)
        {
            return num1 + num2;
        }


    }
}
